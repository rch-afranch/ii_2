Функция ПолучитьКнигиПоИздательству(Издательство) Экспорт
    
    АдресСервиса =
        "http:/localhost/II_1_RCH/ws/BooksService.1cws?wsdl";
    
    WS = Новый WSПрокси(АдресСервиса);
    
    JSONСтрока = WS.GetBooksByPublisher(Издательство);
    
    ЧтениеJSON = Новый ЧтениеJSON;
    ЧтениеJSON.УстановитьСтроку(JSONСтрока);
    
    Таблица = ПрочитатьJSON(ЧтениеJSON);
    
    Возврат Таблица;
    
КонецФункции